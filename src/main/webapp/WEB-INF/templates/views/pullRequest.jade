-@ val checklist: prchecklist.models.ReleaseChecklist

h1 #{checklist.pullRequest.title}

.description
  | blah blah blah
  | meh meh meh

ul#checklist
  = checklist.checks.map
    - case (nr, check) =>
      li
        a(href="#") ##{nr}
        | なんとか機能
        |  #{check.checkedUsers}
        form(method="post" action="/#{checklist.pullRequest.repo.fullName}/pull/#{checklist.pullRequest.number}/check/#{check.pullRequestNumber}")
          button.btn(type="submit" is="ChecklistCheckButton" data-repo={checklist.pullRequest.repo.fullName} data-pr={checklist.pullRequest.number} data-feature-pr={check.pullRequestNumber}) +
